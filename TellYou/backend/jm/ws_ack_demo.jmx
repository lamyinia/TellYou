            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">def messageId = String.valueOf(System.currentTimeMillis())
def ts = System.currentTimeMillis()
def V = { k, d=null -&gt; vars.get(k) ?: props.get(k) ?: d }

def payload = [
  messageId: messageId,
  type     : 1,
  fromUId  : V(&apos;fromUId&apos;),
  toUserId : V(&apos;toUserId&apos;),
  sessionId: V(&apos;sessionId&apos;),
  content  : V(&apos;content&apos;,&apos;goTobetter&apos;),
  timestamp: ts,
  extra    : [platform:&apos;desktop&apos;]
]
vars.put(&apos;payload&apos;, new groovy.json.JsonBuilder(payload).toString())</stringProp>
          </JSR223PreProcessor>
